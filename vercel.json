{
  "functions": {
    "api/index.js": { "runtime": "nodejs18.x", "memory": 512, "maxDuration": 10 }
  },
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/dist",
  "rewrites": [
    { "source": "/api", "destination": "/api/index.js" },
    { "source": "/api/(.*)", "destination": "/api/index.js" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}